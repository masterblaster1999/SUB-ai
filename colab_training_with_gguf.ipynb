    {
      "cell_type": "code",
      "source": [
        "# üß™ Step 7: Test the model\n",
        "from transformers import pipeline\n",
        "\n",
        "print(\"="*60)\n",
        "print(\"TESTING MODEL\")\n",
        "print(\"="*60)\n",
        "\n",
        "generator = pipeline(\n",
        "    'text-generation',\n",
        "    model='./sub_ai_model',\n",
        "    tokenizer=tokenizer,\n",
        "    device=0  # Use GPU\n",
        ")\n",
        "\n",
        "test_prompts = [\n",
        "    \"User: Hello!\\nAssistant:\",\n",
        "    \"User: What is AI?\\nAssistant:\",\n",
        "    \"User: How are you?\\nAssistant:\"\n",
        "]\n",
        "\n",
        "for prompt in test_prompts:\n",
        "    print(f\"üí¨ {prompt}\")\n",
        "    result = generator(\n",
        "        prompt,\n",
        "        max_length=100,\n",
        "        num_return_sequences=1,\n",
        "        temperature=0.8,\n",
        "        do_sample=True,\n",
        "        pad_token_id=tokenizer.eos_token_id\n",
        "    )\n",
        "    response = result[0]['generated_text'].replace(prompt, \"\").strip()\n",
        "    print(f\"   {response}\\n\")\n",
        "\n",
        "print(\"="*60)"
      ],
      "metadata": {
        "id": "test_model"
      },
      "execution_count": null,
      "outputs": []
